# will update
class Solution:
    def removeElement(self, nums: List[int], val: int) -> int:
        k = 0
        for i in range(len(nums)):
            if nums[i] != val:
               nums[k] = nums[i]
               k += 1
        return k

# Time Complexity : O(n)
# Space Complexity : O(1)
# by ar - sayeem [January 25, 2026]
